
public class UserMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String emp_no = "A-1001";
		String id = "java";
		String pwd = "5678";
		String name = "뒙프로그램";
		String rnumber = "7809012621913";
		String dept = "솔루션개발센터";
		String position = "부장";
		String registdate = "20020408";
		
		//xi 1:insert 2:update 3:delete 4:select 5:selectall
		int xi = 1;
		UserInfoVO returnVo = null;
		UserInfoVO vo = new UserInfoVO(emp_no, id, pwd, name, rnumber, dept, position, registdate);
		try {
			if(xi == 1) {
				vo.setCommand("insert");
				UserControl ctrl = new UserControl(vo);
				ctrl.send(vo);
			}
			if(xi == 2) {
				vo.setCommand("update");
				UserControl ctrl = new UserControl(vo);
				ctrl.send(vo);
			}
			
			if(xi == 3) {
				vo.setCommand("delete");
				UserControl ctrl = new UserControl(vo);
				ctrl.send(vo);
			}
			
			if(xi == 4) {
				vo.setCommand("select");
				UserControl ctrl = new UserControl(vo);
				returnVo = ctrl.send(vo);
			}
			
			System.out.println("사번: " + returnVo.getEmp_no() + "\n");
			System.out.println("아이디: " + returnVo.getId() + "\n");
			System.out.println("패스워드: " + returnVo.getPwd() + "\n");
			System.out.println("이름: " + returnVo.getName() + "\n");
			System.out.println("주민번호: " + returnVo.getRnumber() + "\n");
			System.out.println("부서: " + returnVo.getDept() + "\n");
			System.out.println("직위: " + returnVo.getPosition() + "\n");
			System.out.println("등록일자: " + returnVo.getRegistdate() + "\n");
			System.out.println("조회가 정상적으로 수행되었습니다.");
		}
	}

}
